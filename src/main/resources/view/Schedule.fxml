<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="placeHolder" styleClass="pane-with-border" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER">
            <children>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekdayLabel" text="Mon, " />
                        <Label fx:id="monDate" styleClass="weekdayLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="monListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekdayLabel" text="Tue, " />
                        <Label fx:id="tueDate" styleClass="weekdayLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="tueListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekdayLabel" text="Wed, " />
                        <Label fx:id="wedDate" styleClass="weekdayLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="wedListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekdayLabel" text="Thu, " />
                        <Label fx:id="thuDate" styleClass="weekdayLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="thuListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekdayLabel" text="Fri, " />
                        <Label fx:id="friDate" styleClass="weekdayLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="friListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekendLabel" text="Sat, " />
                        <Label fx:id="satDate" styleClass="weekendLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="satListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="5" left="15" right="5" top="5" />
                    </padding>
                    <children>
                  <HBox>
                     <children>
                              <Label styleClass="weekendLabel" text="Sun, " />
                        <Label fx:id="sunDate" styleClass="weekendLabel" text="\$Date" />
                     </children>
                  </HBox>
                  <ListView fx:id="sunListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</StackPane>
